  "license": "MIT",
  "keywords": [
    "javascript",
    "programming",
    "node.js"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HowProgrammingWorks/Labs.git"
  },
  "bugs": {
    "url": "https://github.com/HowProgrammingWorks/Labs/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "funding": "https://www.patreon.com/tshemsedinov",
  "main": "./bin/hpw.js",
  "bin": {
    "hpw": "bin/hpw.js"
  },
  "scripts": {
    "test": "npm run lint && node ./bin/hpw.js",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier --check \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "concolor": "^1.0.6"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0"
 }
}
